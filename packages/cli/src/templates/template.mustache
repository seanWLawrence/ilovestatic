// @flow

import React, { Component } from 'react';
import { StaticQuery, graphql } from 'gatsby';

type Props = {
  {{#props}}
  {{name}}: {{type}}, 
  {{/props}}
  data: any
};

export default class {{name}} extends Component<Props, {}> {

  render() {
    let {
      {{#props}}
      {{name}},
      {{/props}}
      data
    } = this.props;

    return (
      <StaticQuery
        query={graphql`
          query NewQuery {}
        `}
        render={data => (
          <>
            New component
          </>
        )}
      />
    )
  }
};